extends layout

block content
  div
    h1= title
    p Bienvenido a #{title}
  div#mapa

    script.
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('mapa'), {
        center: {lat: -34.603889, lng: -58.382333},
        zoom: 15
        });
      }
    script(async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCpI1dm4HaiIrP5v4B9SNtQ6xdJxNlhwWs&v=3&callback=initMap")
  

  